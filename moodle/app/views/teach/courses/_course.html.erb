<div id="<%= dom_id course %>">
  <%= render "attachment",course:course %>
  <h5 class="card-title" style="text-align: left;margin-top: 20px;margin-left: 10px" ><%=course.name %></h5>
  <div class="card-body">
    <p class="card-text " ><%= course.description.slice(0, 50) %>
      <%= link_to "...show more", teach_course_path(course.id) %>
    </p>
    <a href="<%=teach_course_assignments_path(course.id) %>"  class="btn btn-primary">course assignments</a>
  </div>

</div>